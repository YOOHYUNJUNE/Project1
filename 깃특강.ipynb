   "source": [
    "## 함수 선언부\n",
    "def add_func(n1, n2):\n",
    "    return n1 + n2\n",
    "\n",
    "def mul_func(n1, n2):\n",
    "    return n1 * n2\n",
    "\n",
    "\n",
    "## 전역 변수부\n",
    "num1, num2 = 100, 200\n",
    "result = 0\n",
    "\n",
    "\n",
    "\n",
    "\n",
    "# 메인 코드부\n",
    "result = add_func(num1, num2)\n",
    "print(num1, '+', num2, '=', result)\n",
    "\n",
    "result = mul_func(num1, num2)\n",
    "print(num1, 'x', num2, '=', result)\n"
   ]
  },
